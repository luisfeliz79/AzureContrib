{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}",
            "vmArgs": "-javaagent:applicationinsights-agent-3.4.12.jar"
        },
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "com.felizlabs.samples.telemetrytagging.App",
            "projectName": "felizlabs-samples-appinsights-telemetry-tagging",
            "vmArgs": "-javaagent:applicationinsights-agent-3.4.12.jar"
        },
        {
            "type": "java",
            "name": "App without App Insights Agent",
            "request": "launch",
            "mainClass": "com.felizlabs.samples.telemetrytagging.App",
            "projectName": "felizlabs-samples-appinsights-telemetry-tagging"
        },
        {
            "name": "Python: File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "justMyCode": true
        }
    ]
}